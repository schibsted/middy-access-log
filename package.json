{
  "name": "@schibsted/middy-access-log",
  "version": "4.0.2",
  "description": "Middy middleware for logging processed requests in an access-log like fashion. Optimised for JSON loggers e.g. Bunyan or Pino.",
  "main": "index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint-fix": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "husky install",
    "prettier": "prettier --write",
    "release": "release-it",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "npm run prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schibsted/middy-access-log.git"
  },
  "keywords": [
    "Lambda",
    "Middleware",
    "Serverless",
    "Framework",
    "AWS",
    "AWS Lambda",
    "Middy",
    "Access log",
    "Logging",
    "bunyan",
    "pino",
    "JSON logger"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schibsted/middy-access-log/issues"
  },
  "homepage": "https://github.com/schibsted/middy-access-log#readme",
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@middy/core": "3.6.2",
    "@release-it/conventional-changelog": "5.1.1",
    "eslint": "8.23.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "http-errors": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "release-it": "15.6.0"
  },
  "dependencies": {
    "ramda": "^0.28.0"
  },
  "peerDependencies": {
    "@middy/core": ">=2.0.0"
  }
}
